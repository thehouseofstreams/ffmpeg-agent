# FFmpeg binary (default: ffmpeg in PATH)
FFMPEG_PATH=ffmpeg

# Agent listen port
PORT=8080

# Comma-separated regex patterns allowed for input URLs (optional)
ALLOWED_INPUT_PATTERNS=.*

# Optional CORS origin override (comma list)
CORS_ORIGINS=http://localhost:3000

# Deshabilitar logs de FFmpeg para ahorrar espacio
DISABLE_FFMPEG_LOGS=true